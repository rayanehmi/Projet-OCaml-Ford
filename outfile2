digraph finite_state_machine {
rankdir=LR;
size="9"
node [shape = star];
-1 -> 3 [label = "0"];
-1 -> 2 [label = "0"];
2 -> 1 [label = "99990"];
2 -> 3 [label = "100000"];
2 -> -1 [label = "10"];
3 -> -1 [label = "10"];
3 -> 1 [label = "99990"];
3 -> 2 [label = "100000"];
1 -> 2 [label = "100010"];
1 -> -2 [label = "0"];
1 -> 3 [label = "100010"];
-2 -> 1 [label = "20"];
}
